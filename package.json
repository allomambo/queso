{
    "name": "@allomambo/tacos-toppings",
    "description": "",
    "public": true,
    "version": "0.0.2",
    "engines": {
        "node": ">=16.14.2",
        "npm": ">=8.0.0"
    },
    "repository": "https://github.com/allomambo/tacos-toppings.git",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/allomambo/tacos-toppings/issues"
    },
    "homepage": "https://github.com/allomambo/tacos-toppings#readme",
    "type": "module",
    "files": [
        "dist/*",
        "src/*"
    ],
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./css": "./dist/style.css"
    },
    "scripts": {
        "ci": "yarn install --immutable --immutable-cache --check-cache",
        "vite": "vite",
        "watch-tsc": "vue-tsc --watch --noEmit",
        "serve": "npm-run-all --parallel watch-tsc vite",
        "build:lib": "vite build",
        "publish:lib": "npm publish --access public",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "storybook": "start-storybook -p 6006",
        "build:storybook": "build-storybook -o ./docs -c ./.storybook",
        "test:coverage": "vitest run --coverage",
        "test:run": "vitest --run",
        "test": "vitest"
    },
    "dependencies": {
        "@vitejs/plugin-legacy": "^3.0.1",
        "@vueuse/core": "^9.12.0",
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@storybook/builder-vite": "^0.4.0",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "@vue/test-utils": "^2.2.7",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-markdown": "^2.2.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-vue": "^8.0.1",
        "happy-dom": "^3.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.0",
        "sass": "^1.44.0",
        "terser": "^5.16.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-restart": "^0.3.0",
        "vitest": "^0.27.2",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.11"
    },
    "overrides": {
        "@mdx-js/react": {
            "react": "^18.2.0",
            "react-dom": "^18.2.0"
        }
    }
}
