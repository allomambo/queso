{
    "name": "@allomambo/queso",
    "version": "0.0.3",
    "description": "VueJS component library to use in conjunction with Arepa",
    "repository": "https://github.com/allomambo/queso.git",
    "author": "MamboMambo <allo@mambomambo.ca> (https://mambomambo.ca)",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.js",
    "files": [
        "dist/*",
        "src/*"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./styles/components": "./dist/style.css",
        "./styles/utils": "./src/assets/main.scss",
        "./styles/reset": "./src/assets/00.core/_ress.scss",
        "./scripts/utils": "./src/utils/index.ts"
    },
    "engines": {
        "node": ">=16.14.2",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "ci": "yarn install --immutable --immutable-cache --check-cache",
        "vite": "vite",
        "watch-tsc": "vue-tsc --watch --noEmit",
        "serve": "npm-run-all --parallel watch-tsc vite",
        "build:lib": "vite build",
        "publish:lib": "npm publish --access public",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "test:coverage": "vitest run --coverage",
        "test:run": "vitest --run",
        "test": "vitest"
    },
    "dependencies": {
        "@vueuse/core": "^9.12.0",
        "vue": "^3.2.45"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vitejs/plugin-legacy": "^3.0.1",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "@vue/test-utils": "^2.2.7",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-markdown": "^2.2.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-vue": "^8.0.1",
        "happy-dom": "^3.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.0",
        "sass": "^1.44.0",
        "terser": "^5.16.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-restart": "^0.3.0",
        "vitest": "^0.27.2",
        "vue-eslint-parser": "^9.1.0",
        "vue-tsc": "^1.0.11"
    },
    "overrides": {
        "@mdx-js/react": {
            "react": "^18.2.0",
            "react-dom": "^18.2.0"
        }
    }
}
